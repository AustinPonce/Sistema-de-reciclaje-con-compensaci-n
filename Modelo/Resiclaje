public abstract class Resiclaje{//star class
    //atrivutos
protected String name,id;
protected double kg;
    
//star met abst
public Resiclaje (String name,String id, double kg){
    this.name=name;
    this.id=id;
    this.kg=kg;
}
//end met abst
// gets and sets

public String getName(){
    return name;
}
public void String setName(String name){
    this.name=name;
}

public String getId(){
    return id;
}
public void String setId(String id){
    this.id=id;
}
public double getKg(){
    return kg;
}
public double setKg(double kg){
    this.kg=kg;
}
//end gets and sets

@Override
public  String toString(){

    return "Informacion \r\n"
    +"Nombre: "+ name 
    +"\nID: " + id
    + "\nKg: "+ kg;

}//end override

}//end class